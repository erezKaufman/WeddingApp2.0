<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Anonymous_user_entry"
    tools:layout_editor_absoluteY="81dp">

    <ImageView
        android:id="@+id/weedingHello"
        android:layout_width="258dp"
        android:layout_height="0dp"
        android:layout_marginBottom="181dp"
        android:layout_marginTop="3dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weedingHello">

        <Button
            android:id="@+id/gotosignin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="חפשני"/>
        <!--<Button-->
            <!--android:id="@+id/gotosignin"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="@string/click_to_sign_in" />-->

        <Button
            android:id="@+id/gotoSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/gotosearch" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>